{
  "name": "color-info",
  "displayName": "Color Info",
  "description": "Text to speech",
  "version": "0.0.0",
  "publisher": "bierner",
  "license": "MIT",
  "keywords": [
    "color",
    "rgb"
  ],
  "icon": "media/logo.svg",
  "galleryBanner": {
    "color": "#fefefe",
    "theme": "light"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
